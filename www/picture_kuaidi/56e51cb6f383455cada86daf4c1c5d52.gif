            $(this).removeClass("hover");
        });
        //清空单号
        $("#cleanbill").click(function () {
            $("#txtbill").val("");
        });
    });
    function checkBillData() {
        var arrChk = $("input[name^=checkbox]:checked");
        if (arrChk.length <= 0) {
            if (checkBill("1")) {
                $("#formbill").prop("action", "/GuestService/BillNew");
                document.formbill.submit();
            } else {
                return false;
            }
        } else {
            if (checkBill("0")) {
                $("#formbill").prop("action", "/GuestService/InternationalBillQuery");
                document.formbill.submit();
            } else {
                return false;
            }
        }
    };
    //验证单号
    function checkBill(type) {
        var txtbill = document.getElementById("txtbill");
        if (txtbill.value.trim() == "") {
            alert("请输入运单号码 !!");
            return false;
        }
        var listI = txtbill.value.trim().split("\r\n");
        var listF = txtbill.value.trim().split("\n");
        var list = null;
        if (listI.length > listF.length) {
            list = listI;
        } else {
            list = listF;
        }
        if (list.length > 20) {
            alert('一次最多只能查询20个快件单号');
            return false;
        }
        var msg = "";
        if (type == "1") {

            for (i = 0; i < list.length; i++) {
                var len = i + 1;
                var bill = sTrim(list[i].trim(), "g");
                if (bill.length != 12 && bill.length != 0) {
                    msg += '第' + len + '个快件单号[' + bill + ']长度必须等于12!\n';
                }
                else if (isNaN(bill)) {
                    msg += '第' + len + '个快件单号必须是数字,且为半角输入法!\n';
                }
            }
        }
        if (msg != "") {
            alert("亲，出现错误啦！\r\n" + msg);
            return false;
        } else {
            return true;
        }
    }
    function sTrim(str, isglobal) {
        var result;
        result = str.replace(/(^\s+)|(\s+$)/g, "");
        if (isglobal.toLowerCase() == "g")
            result = result.replace(/\s/g, "");
        return result;
    }
</script>



